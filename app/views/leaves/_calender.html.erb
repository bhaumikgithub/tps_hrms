<div id="app-calendar">
    <div class="row">
        <div class="col s12">
            <div class="card">
                <div class="card-content">
                    <h4 class="card-title">
                    </h4>
                    <div id="calendar"></div>
                </div>
            </div>
        </div>
    </div>
</div>

<script>
    $(document).ready(function() {
        $('#calendar').fullCalendar({
          defaultDate: '2019-08-12',
          editable: true,
          eventLimit: true,
          events: '/leaves/get_events',
        });
    });
  
</script>
